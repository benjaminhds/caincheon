.sec03 > div > ul li:nth-child(4n-3){margin-left:0;}
.sec03 > div > ul li a{display:table;width:100%; height:100%;  position:relative; overflow:hidden; }
.sec03 > div > ul li a > em{position:absolute; left:0; top:-30px;  z-index: 0; transition:all 1s; -moz-transition:all 1s; -webkit-transition:all 1s; -o-transition:all 1s;  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)"; -moz-opacity:0;-khtml-opacity:0; opacity:0; width:100%; height:100%;}
.sec03 > div > ul li a:hover > em{ top: 0;   -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)"; -moz-opacity:0.5;-khtml-opacity:0.5; opacity:0.5;}
.sec03 > div > ul li a > em img{width:100%; height:100%;}
.sec03 > div > ul li a i{display:table-cell; vertical-align: middle ; position:relative ; z-index:1;}
.sec03 > div > ul li a i img{margin:0 auto;  transition:all 1s; -moz-transition:all 1s; -webkit-transition:all 1s; -o-transition:all 1s; -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)"; -moz-opacity:0.8;-khtml-opacity:0.8; opacity:0.8;}
.sec03 > div > ul li a:hover i img{transform: translateY(-10px); -moz-transform: translateY(-10px); -o-transform: translateY(-10px); -webkit-transform: translateY(-10px); -ms-transform: translateY(-10px); -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; -moz-opacity:1;-khtml-opacity:1; opacity:1;}


.sec03 > div > ul li a span{display:block; font-size:22px; color:#fff; letter-spacing:-0.75px; margin-top:20px; transition:all 0.5s; -moz-transition:all 0.5s; -webkit-transition:all 0.5s; -o-transition:all 0.5s;  padding:0 5%; line-height:1.5; word-break:keep-all;}

.sec03 > div > .box{height:360px; width:100%; margin-top:20px; position:relative; background-image: url('/img/main/sec_03_img.jpg'); overflow-x:hidden;background-size:cover; padding:40px 60px; }
.sec03 > div > .box h4{font-size:26px; color:#333333; font-weight:bold; letter-spacing: -0.75px; font-weight:bold;}
.sec03 > div > .box span{font-size:18px; color:#666666; line-height:1.5; letter-spacing: -0.75px; padding-top:15px; display:block; }
.sec03 > div > .box ul{display:block; margin-top:20px;}
.sec03 > div > .box ul li{float:left; margin-top:10px; margin-left:10px;}
.sec03 > div > .box ul li:nth-child(4n-3){clear:both; margin-left:0;}
.sec03 > div > .box ul li a{display:table-cell; vertical-align: middle;color:#cd4400; font-size:18px; font-weight:bold; letter-spacing:-0.75px; background-image:url('/img/main/sec_03_box.png');  width:138px; height:38px; text-align: center;  transition:all 1s; -moz-transition:all 1s; -webkit-transition:all 1s;
-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=70)"; -moz-opacity:0.7;-khtml-opacity:0.7; opacity:0.7;
}
.sec03 > div > .box ul li a:hover{-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; -moz-opacity:1;-khtml-opacity:1; opacity:1; }

.sec04{width:100%; height: 511px; position:relative;  padding:80px 0;background-size:cover; background-image: url('/img/main/sec_04_bg.jpg'); background-position:100% ; z-index:2;}
.sec04 > article{width:1400px; margin:0 auto; display:block;}
.sec04 > article h4{font-size:26px; color:#ffffff;  letter-spacing:-0.75px; font-weight:normal; text-align: center;}
.sec04 > article .bx-viewport{padding-top:70px; overflow:visible;}
.sec04 > article .bx-viewport > ul{width:100%; /* margin-top:20px;  */display:table; font-size:0}
.sec04 > article .bx-viewport > ul > li{display:inline-block; vertical-align: top; width:451px; height:170px; background-image:url('/img/main/sec_04_01_bg.png'); position:relative;  transition:all 1s; -moz-transition:all 1s; -webkit-transition:all 1s; -o-transition:all 1s; background-size:100% 100%;}
/* .sec04 > article .bx-viewport > ul > li ~ li{margin-left:3%} */
