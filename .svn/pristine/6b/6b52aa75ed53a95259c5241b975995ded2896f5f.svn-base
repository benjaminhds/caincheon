	function getYearData(num, chk="") {
		is_loaded = false;
		
		if (chk!="")
		{
			alert("dddd");
		}
		var url = "history_years_"+num+".html";

		$.ajax({
			url: url,
			cache: false,	
			data: {},
			type: "POST",
			dataType: "html",				
			success: function(val) {	
				$(".years").empty();	
				$(".years").append(val);
				is_loaded = true;			
			},
			error: function(request,status,error) {
				alert("error");
				//console.log("code:"+request.status+"\n"+"message:"+request.responseText+"\n"+"error:"+error);
			}				
		});
	}


	$(function() {
