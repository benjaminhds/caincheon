<script type="text/javascript">
var ajaxChangeBody = function(fullURL) {
	$.ajax({
		type: 'GET',
		url: fullURL,
		async: false,
		dataType: "text",
		contentType:"application/x-www-form-urlencoded; charset=UTF-8",
		success: function(data) {
			$('body').html(data);
		},
		error: function(request, status, error) {
			alert(error);
		}
	});
};
</script>
